# kubernetes/apps/default/semaphoreui/ingressroute.yaml
apiVersion: traefik.io/v1alpha1
kind: IngressRoute
metadata:
  name: semaphoreui
  namespace: kube-system
spec:
  entryPoints:
    - websecure
  routes:
    - match: Host(`semaphoreui.torquasmvo.com`)
      kind: Rule
      services:
        - name: semaphoreui
          namespace: default
          port: 3000
  tls:
    secretName: traefik-dashboard-tls
